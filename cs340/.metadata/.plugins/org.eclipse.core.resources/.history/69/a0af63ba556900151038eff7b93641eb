import java.io.BufferedReader;
import java.io.IOException;
import java.io.InputStreamReader;
import java.util.Scanner;


public class Solution {

	
	/**
	 * Take a rectangular grid of numbers and find the length
	 * of the longest non-decreasing sub-sequence.
	 * @return the length as an integer.
	 */
	public static int longestSequence(int[][] grid) {
        String[][] bestNextNum = new String[numRow][numCol];
		
        int min = grid[1][1];
        
        for (int i = 0; i < grid.length; i++) {
            for (int j = 0; j < grid[i].length; j++) {
            	
            	//Find the min number on the board
            	if (grid[i][j] < min)
            		min = grid[i][j];
            	
            	int currentBest = grid[i][j];

                //Find the best number to follow the current number
                
                //int nextBestNum = grid[i][j];
                if (i > 0) {
                	//W
                	if (grid[i-1][j] >= currentBest) {
                		bestNextNum[i][j] = "W";
                		currentBest = grid[i-1][j];
                	}
                	if (j > 0) {
                		//NW
                		if (grid[i-1][j-1] >= currentBest) {
                			bestNextNum[i][j] = "NW";
                			currentBest = grid[i-1][j-1];
                		}
                	}
                	if (j < grid[i].length-1) {
                		//SW
                		if (grid[i-1][j+1] >= currentBest) {
                			bestNextNum[i][j] = "SW";
                			currentBest = grid[i-1][j+1];
                		}
                			
                	}
                }
                if (i < grid.length-1) {
                	if (grid[i+1][j] >= currentBest) {
                		bestNextNum[i][j] = "E";
                		currentBest = grid[i+1][j];
                	}
                	if (j > 0) {
                		if (grid[i+1][j-1] >= currentBest) {
                			bestNextNum[i][j] = "NE";
                			currentBest = grid[i+1][j-1];
                		}
                	}
                	if (j < grid[i].length-1) {
                		if (grid[i+1][j+1] >= currentBest) {
                			bestNextNum[i][j] = "SE";
                			currentBest = grid[i+1][j+1];
                		}
                	}
                }
                if (j > 0) {
                	if (grid[i][j-1] >= currentBest) {
                		bestNextNum[i][j] = "N";
                		currentBest = grid[i][j-1];
                	}
                }
                if (j < grid[i].length-1) {
                	if (grid[i][j+1] >= currentBest) {
                		bestNextNum[i][j] = "S";
                		currentBest = grid[i][j+1];
                	}
                }
                	
            }
            
            
        }
        
        
		return 0;
	}

	public static void main(String[] args) throws IOException {
		BufferedReader reader = new BufferedReader(new InputStreamReader(System.in));

		int numRows = 0;
		int numCols = 0;
		String[] firstLine = reader.readLine().split("\\s+");
		numRows = Integer.parseInt(firstLine[0]);
		numCols = Integer.parseInt(firstLine[1]);

		numRow = numRows;
		numCol = numCols;
		
		int[][] grid = new int[numRows][numCols];

		for (int row = 0; row < numRows; row++) {
		    String[] inputRow = reader.readLine().split("\\s+");

		    for (int col = 0; col < numCols; col++) {
		        grid[row][col] = Integer.parseInt(inputRow[col]);
		    }
		}
		int length = longestSequence(grid);
		System.out.println(length);
	}
	
	public static int numRow;
	public static int numCol;

}
